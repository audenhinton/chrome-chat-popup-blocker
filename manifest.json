{
    "name": "No Chatbot",
    "description": "A tiny extension to block chat popup windows while browsing websites.",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": [
        "activeTab", "scripting"
    ],
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png"
      },
    "background": {
        "service_worker": "plugin.js"
    }
}